{
    "name": "nigelheap/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 5.3 and 5.6",
    "keywords": ["roles", "permissions", "acl", "auth", "laravel", "illuminate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nigel Heap",
            "email": "nigel@nigelheap.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.6"
    },
    "autoload": {
        "psr-0": {
            "NigelHeap\\Roles": "src/"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "NigelHeap\\Roles\\RolesServiceProvider"
          ],
          "aliases": {
              "Role": "NigelHeap\\Roles\\Models\\Role",
              "Permission": "NigelHeap\\Roles\\Models\\Permission"
          }
      }
    }
}
